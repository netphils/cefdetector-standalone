{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cefdetector-standalone",
  "version": "0.1.1",
  "identifier": "com.cefdetector-standalone.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "cefdetector-standalone",
        "width": 1000,
        "height": 749
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
